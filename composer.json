{
    "name": "juzdy/juzdy",
    "description": "Skibidi Madness web",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "juzdy/core": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src/"
        }
    },
    "config": {
        "optimize-autoloader": true,
        "sort-packages": true,
        "allow-plugins": {
            "cweagans/composer-patches": true
        }
    },
    "require-dev": {
        "cweagans/composer-patches": "^2.0"
    },
    "extra": {
        "patches": {
            "juzdy/core": {
                "Fix pagination count query parameter binding": "patches/fix-pagination-count.patch"
            }
        }
    }
}
